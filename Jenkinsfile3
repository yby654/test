#!/usr/bin/env groovy
import hudson.model.*

node {
    stage('Docker Build image') {
        docker.withRegistry("https://registry.hub.docker.com", 'docker-id-hub') {
            sh 'docker build -t yby654/demo:latest -f ./Dockerfile .'
        }
    }
    stage('Docker Push image') {
        docker.withRegistry("https://registry.hub.docker.com", 'docker-id-hub') {
            docker.image('yby654/demo:latest').push()
        }
    }
}